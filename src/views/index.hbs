<header>
    <h1 id="homeHeader" class="wesiteHeader">Genshin Impact Data</h1>
</header>

{{#if user}}
<div><h3>Currently Tracked Characters:</h3></div>
<ul>
    {{#each characters}}
    <li>{{this.name}}</li>
    {{/each}}
</ul>

<div><h3>Currently Tracked Weapons:</h3></div>
<ul>
    {{#each weapons}}
    <li>{{this.name}}</li>
    {{/each}}
</ul>

{{/if}}

